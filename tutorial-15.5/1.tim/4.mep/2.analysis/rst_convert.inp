# control parameters in rst_convert

[INPUT]
rstfile        = ../1.string16/NAME_NUM.rst
pdbfile        = ../../2.equil/step4_nvt_100.pdb
psffile        = ../../2.equil/step4_nvt_100.psf

[OUTPUT]
pdbfile        = NAME_NUM.pdb

[OPTION]
check_only     = NO              # only checking input files (YES/NO)
allow_backup   = NO              # backup existing output files (YES/NO)
rstin_format   = GENESIS         # (NAMD/GENESIS)
rstout_format  = PDB             # (NAMD/GENESIS/PDB/CHARMM/AMBER)
rstout_type    = AUTO            # (AUTO/MIN/MD/REMD)
pbc_correct    = NO              # (NO/MOLECULE)
